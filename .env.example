# API Keys
NEXT_PUBLIC_VEDIC_ASTRO_API_KEY=your_api_key_here
NEXT_PUBLIC_PROKERALA_API_KEY=your_api_key_here

# Google Maps API Key (for Places Autocomplete)
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable these APIs: Places API, Maps JavaScript API, Geocoding API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Database (if using Supabase)
DATABASE_URL=your_database_url
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_key

# Redis Cache (if using Upstash)
REDIS_URL=your_redis_url
REDIS_TOKEN=your_redis_token

# NextAuth Configuration
NEXTAUTH_SECRET=your-super-secret-key-here-min-32-chars
NEXTAUTH_URL=http://localhost:3000

# OAuth Providers (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
